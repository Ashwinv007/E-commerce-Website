<section>
    <div class="container">
        <table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
    <th scope="col">Category</th>
      <th scope="col">Price</th>

    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">this</th>
      <td>{{this.name}}</td>
      <td>{{this.description}}</td>
      <td>{{this.category}}</td>
            <td>{{this.price}}</td>
                  <td><img style="width: 50px; height 50px" src="{{this.image}}" alt=""></td>


    </tr>
    {{/each}}
  

  </tbody>
</table>
    </div>
</section>